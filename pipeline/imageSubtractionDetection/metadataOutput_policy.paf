RunMode: preprocess
AdditionalData: "exposureId=triggerVisitEvent.exposureId"
OutputItems: {
    visitMetadata: {
        Required: true
        StoragePolicy: {
            Storage: "DbStorage"
            Location: "mysql://lsst10.ncsa.uiuc.edu:3306/%{runId}"
	}
    }
}

Persistence: {
    Formatter: {
        DataProperty: {
            visitMetadata: {
                TableName: "Raw_FPA_Exposure"
                KeyList: "rawFPAExposureId" "ra" "decl" "equinox"
                KeyList: "filterId" "dateObs" "expTime" "airmass"
            }
        }
    }
}
